{
  "suggest.noselect": true,
  "snippets.ultisnips.pythonPrompt": false,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls-root", "compile_commands.json"],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        },
        "client": {
          "snippetSupport": true
        }
      }
    }
  },
  "cSpell.userWords": [
    "GLBL",
    "Hotspot",
    "Idoc",
    "MIMEMAP",
    "Mockplus",
    "STRIKETHROUGH",
    "artboard",
    "consts",
    "elipse",
    "guids"
  ]
}
